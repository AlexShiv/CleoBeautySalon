<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Time.
    -->
    <changeSet id="20200115182335-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="time"
                                 constraintName="fk_time_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="job_id"
                                 baseTableName="time"
                                 constraintName="fk_time_job_id"
                                 referencedColumnNames="id"
                                 referencedTableName="job"/>

        <addForeignKeyConstraint baseColumnNames="salon_id"
                                 baseTableName="time"
                                 constraintName="fk_time_salon_id"
                                 referencedColumnNames="id"
                                 referencedTableName="salon"/>

    </changeSet>
</databaseChangeLog>
